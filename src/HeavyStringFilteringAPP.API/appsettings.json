{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Filtering": {
    "AlgorithmDescription": "Replace Levenshtein or JaroWinkler  on Algoritm options",
    "Algorithm": "Levenshtein",
    "SimilarityThresholdLevenshtein": 0.82,
    "SimilarityThresholdJaroWinkler": 0.75,
    "BlackListPath": "src\\assets\\blacklist.txt"
  },
  "AllowedHosts": "*"
}